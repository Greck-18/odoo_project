<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="polish_test2_form_view" model="ir.ui.view">
        <field name="name">polish.test2.form</field>
        <field name="model">polish.test2</field>
        <field name="arch" type="xml">
            <form string="Polish Test2">
                <sheet>
                    <group>
                            <field name="text"/>
                            <field name="check1"/>
                            <field name="check2"/>
                            <field name="check_all"/>
                            <field name="select1"/>
                            <field name="select2"/>

                            <field name="boolean1" attrs="{'invisible':[('select1','in',['2','3',False]),('select2','in',['5','6',False])]}"/>
                            <field name="boolean2" attrs="{'invisible':[('select1','in',['2','3',False]),('select2','in',['4','6',False])]}"/>
                            <field name="boolean3" attrs="{'invisible':[('select1','in',['2','3',False]),('select2','in',['5','4',False])]}"/>
                            <field name="boolean4" attrs="{'invisible':[('select1','in',['1','3',False]),('select2','in',['5','6',False])]}"/>
                            <field name="boolean5" attrs="{'invisible':[('select1','in',['1','3',False]),('select2','in',['4','6',False])]}"/>
                            <field name="boolean6" attrs="{'invisible':[('select1','in',['1','3',False]),('select2','in',['5','4',False])]}"/>
                            <field name="boolean7" attrs="{'invisible':[('select1','in',['2','1',False]),('select2','in',['5','6',False])]}"/>
                            <field name="boolean8" attrs="{'invisible':[('select1','in',['2','1',False]),('select2','in',['4','6',False])]}"/>
                            <field name="boolean9" attrs="{'invisible':[('select1','in',['2','1',False]),('select2','in',['5','4',False])]}"/>

                    </group>
                </sheet>
            </form>
        </field>

    </record>

    <record id="action_polish_test2_list" model="ir.actions.act_window">
        <field name="name">Polish Test2</field>
        <field name="res_model">polish.test2</field>
        <field name="view_mode">form</field>
    </record>

    <menuitem id="menu_polish_test2_list"
              action="action_polish_test2_list"/>




</odoo>